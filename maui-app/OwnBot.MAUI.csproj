<Project Sdk="Microsoft.NET.Sdk.Maui">
  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>net6.0-ios;net6.0-android;net6.0-maccatalyst;net6.0-windows10.0.19041.0</TargetFramework>
    <UseMaui>true</UseMaui>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
  </PropertyGroup>

  <ItemGroup>
    <MauiXaml Include="MainPage.xaml" />
    <Compile Include="MainPage.xaml.cs" />
  </ItemGroup>

</Project>